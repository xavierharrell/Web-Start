{
  "name": "Web-Start",
  "version": "1.0.1",
  "description": "Web starting point project with gulp tasks",
  "repository": {
    "type": "git",
    "url": "git://github.com/xavierharrell/Gulp-Start-Point.git"
  },
  "browser": {
    "Modernizr": "/browsernizr",
    "classie": "./src/javascript/vendor/bower_components/classie/classie.js",
